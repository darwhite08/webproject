<div id="animeVaultDashBoardContainer ">
    <nav class="fixed top-0 z-50 w-full bg-white border-b border-gray-200 dark:bg-gray-800 dark:border-gray-700">
        <div class="px-3 py-3 lg:px-5 lg:pl-3">
            <div class="flex items-center justify-between">
                <div class="flex items-center justify-start rtl:justify-end">
                    <!-- Logo start -->
                    <div class="flex ms-2 md:me-24">
                        <p
                            class="  text-white bg-blue-600 rounded-full py-1 px-2 font-medium">
                            anime unwatched.</p>
                    </div>
                    <!-- Logo end -->
                </div>
                <div class="flex items-center">
                    <div class="flex items-center ms-3">
                        <div>
                            <button type="button"
                                class="flex text-sm bg-gray-800 rounded-full focus:ring-4 focus:ring-gray-300 dark:focus:ring-gray-600">
                                <img class="w-8 h-8 rounded-full" src=<%=user_data.profilePicture %> lt="user photo">
                            </button>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </nav>s

    <!-- side bar start -->
    <aside
        class="fixed top-0  px-4 left-0 z-40 w-64 h-screen pt-20 transition-transform -translate-x-full bg-white border-r border-gray-200 sm:translate-x-0 dark:bg-gray-800 dark:border-gray-700">

        <div class="flex items-center gap-4 mb-6 border-b border-dashed pb-4">
            <img class="w-10 h-10 rounded-full" src=<%=user_data.profilePicture%> alt="">
            <div class="font-medium text-sm dark:text-white">
                <div></div>
                <div class="text-sm text-gray-500 dark:text-gray-400">Joined 2014</div>
            </div>
        </div>

        <div class="h-full px-3 pb-4 overflow-y-auto bg-white dark:bg-gray-800">
            <ul class="space-y-2 font-medium">
                <!-- profile -->
                <li>
                    <a href="/user/dash-board/profile" aria-selected="true"
                        class="flex items-center bg-black text-white px-4 p-2 text-gray-900 rounded-xl dark:text-white group">
                        <i class="fa-solid fa-user"></i>
                        <span class="ms-3">Profile</span>
                    </a>
                </li>
                <!-- upload content -->
                <li>
                    <a href="/user/dash-board/upload-content"
                        class="flex items-center p-2 text-gray-900 rounded-lg dark:text-white hover:bg-gray-100 dark:hover:bg-gray-700 group">
                        <i class="fa-solid fa-cloud-arrow-up"></i>
                        <span
                            class="flex-1 ms-3 me-2 text-md font-medium text-gray-900 focus:outline-none  hover:bg-gray-100 hover:text-blue-700 focus:z-10">Upload Content</span>
                    </a>
                </li>

                <!-- Earning -->

                <li>
                    <a href="/logout"
                        class="flex items-center p-2 text-gray-900 rounded-lg dark:text-white hover:bg-gray-100 dark:hover:bg-gray-700 group">
                        <i class="fa-solid fa-right-to-bracket"></i>
    
                        <span
                            class="flex-1 px-5 me-2 text-md font-medium text-gray-900 focus:outline-none  hover:bg-gray-100 hover:text-blue-700 focus:z-10">Earning</span>
                    </a>
                </li>

                <!-- watchlist -->

                <li>
                    <a href="/user/dash-board/watchlist"
                        class="flex items-center p-2 text-gray-900 rounded-lg dark:text-white hover:bg-gray-100 dark:hover:bg-gray-700 group">
                        <i class="fa-solid fa-list-check"></i>

                        <span
                            class="flex-1 px-5 me-2 text-md font-medium text-gray-900 focus:outline-none  hover:bg-gray-100 hover:text-blue-700 focus:z-10">Watchlist
                        </span>
                    </a>
                </li>
                <!-- read later -->
                <li>
                    <a href="/user/dash-board/read-later    "
                        class="flex items-center p-2 text-gray-900 rounded-lg dark:text-white hover:bg-gray-100 dark:hover:bg-gray-700 group">
                        <i class="fa-solid fa-book-bookmark"></i>
    
                        <span
                            class="flex-1 px-5 me-2 text-md font-medium text-gray-900 focus:outline-none  hover:bg-gray-100 hover:text-blue-700 focus:z-10">Read later
                        </span>
                    </a>
                </li>
                <!-- read by date -->
                <li>
                    <a href="/user/dash-board/calender"
                        class="flex items-center p-2 text-gray-900 rounded-lg dark:text-white hover:bg-gray-100 dark:hover:bg-gray-700 group">
                        <i class="fa-solid fa-calendar"></i>
    
                        <span
                            class="flex-1 px-5 me-2 text-md font-medium text-gray-900 focus:outline-none  hover:bg-gray-100 hover:text-blue-700 focus:z-10">Read by date
                        </span>
                    </a>
                </li>

                <!-- settings -->
                <li>
                    <a id="animeVaultDashboardSettings" href="/user/dash-board/settings"
                        class="flex items-center p-2 text-gray-900 rounded-lg dark:text-white hover:bg-gray-100 dark:hover:bg-gray-700 group">
                        <i class="fa-solid fa-gear"></i>
                        <span
                            class="flex-1 px-5 me-2 text-md font-medium text-gray-900 focus:outline-none  hover:bg-gray-100 hover:text-blue-700 focus:z-10">Settings</span>
                    </a>
                </li>
                <!-- logout -->
                <li>
                    <a href="/logout"
                        class="flex items-center p-2 text-gray-900 rounded-lg dark:text-white hover:bg-gray-100 dark:hover:bg-gray-700 group">
                        <i class="fa-solid fa-right-to-bracket"></i>

                        <span
                            class="flex-1 px-5 me-2 text-md font-medium text-gray-900 focus:outline-none  hover:bg-gray-100 hover:text-blue-700 focus:z-10">Logout</span>
                    </a>
                </li>

            </ul>
            <!-- Notes -->
            <%- include('admin-page-comp/note' ) %>

        </div>

    </aside>
    <!-- side bar end -->
    <div class="px-4 sm:ml-64 sm:pt-20 md:pt-20 lg:pt-20">

        <div
            class="px-4 border-2 border-gray-200 border-dashed rounded-lg flex flex-col items-center justify-center  sm:flex-row md:flex-row lg:flex-row ">

            <% if (profile){%>
                <%- include('admin-page-comp/profile' ,{ profilePicture:profile_picture, username:username,
                    email:email}) %>
                    <% } else if (settings){%>
                        <%- include('admin-page-comp/settings' ) %>
                            <% } else if (content_upload){%>
                                <%- include('admin-page-comp/upload-content' ) %>
                                    <% } else if (watchlist){%>
                                        <%- include('admin-page-comp/watchlist' ) %>
                                            <% } else if (readLater){%>
                                                <%- include('admin-page-comp/read-later' ) %>
                                                    <% } else if (calender){%>
                                                        <%- include('admin-page-comp/calender' ) %>
                                                            <% }%>
        </div>
        <!-- mobile navigation -->
        <nav class="sm:hidden md:hidden lg:hidden block" >
            <div class="py-10  flex ">
                <div class=" px-3 pb-4 overflow-y-auto bg-white ">
                    <ul class="space-y-2 font-medium">
                        <!-- profile -->
                        <li>
                            <a href="/user/dash-board/profile" aria-selected="true"
                                class="flex items-center bg-black text-white px-4 p-2 text-gray-900 rounded-xl  group">
                                <i class="fa-solid fa-user"></i>
                                <span class="ms-3">Profile</span>
                            </a>
                        </li>


                        <li>
                            <a href="/user/dash-board/watchlist"
                                class="flex items-center p-2 text-gray-900 rounded-lg dark:text-white hover:bg-gray-100 dark:hover:bg-gray-700 group">
                                <i class="fa-solid fa-list-check"></i>

                                <span
                                    class="flex-1 px-5 me-2 text-md font-medium text-gray-900 focus:outline-none  hover:bg-gray-100 hover:text-blue-700 focus:z-10">Watchlist
                                </span>
                            </a>
                        </li>
                        <!-- logout -->
                        <li>
                            <a href="/logout"
                                class="flex items-center p-2 text-gray-900 rounded-lg dark:text-white hover:bg-gray-100 dark:hover:bg-gray-700 group">
                                <i class="fa-solid fa-right-to-bracket"></i>

                                <span
                                    class="flex-1 px-5 me-2 text-md font-medium text-gray-900 focus:outline-none  hover:bg-gray-100 hover:text-blue-700 focus:z-10">Logout</span>
                            </a>
                        </li>

                    </ul>
                    <!-- Notes -->
                    <%- include('admin-page-comp/note' ) %>

                </div>
            </div>

        </nav>

    </div>
</div>